<!DOCTYPE html>
<html>
<head>
  <title>Lets530</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>


 
  <link href="/stylesheets/zbootswatch.css" media="screen" rel="stylesheet" type="text/css" />
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
<script src="/javascripts/fieldboxes.js"></script>

<link href="/stylesheets/skeleton.css" media="screen" rel="stylesheet" type="text/css" />



</head>
<body>
	
	<div class="navbar navbar-fixed-top">
	    <div class="navbar-inner">
	      <div class="container">
	        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
	          <span class="icon-bar"></span>
	          <span class="icon-bar"></span>
	          <span class="icon-bar"></span>
	        </a>
	        <a class="brand span2" href="/">LetsDo</a>
	        <div class="nav-collapse">
	          <!-- <form class="navbar-search pull-left" action="/flights"> -->
<%= form_tag(users_url, class: 'navbar-search pull-left', method: 'get') do %>
<%= text_field_tag :search, nil, placeholder: "Search Events", class: 'search-query span3'  %>
	            <!-- <input type="text" class="search-query span3" placeholder="Search by city or airport code"> -->
	          <% end %>
	          <!-- </form> -->
	          <ul class="nav pull-left">
	            <li><%= link_to "Users", users_url %></li>
	            <li><%= link_to "Events", events_url %></li>
				<li><%= link_to "Vote", votes_url %></li>
				 <li><%= link_to "Event_Time", event_times_url %></li>
					<li><%= link_to "Location", locations_url %></li>
	  
	            </li>
	          </ul>
	        </div><!--/.nav-collapse -->
	      </div>
	    </div>
	  </div>

	
	<div class="container">

	    <% if notice.present? %>
	    <div class="alert alert-success">
	      <button type="button" class="close" data-dismiss="alert">&times;</button>
	      <%= notice %>
	    </div>
	    <% end %>
	

<%= yield %>

</body>
</html>
